{
  "slider-layout#slider-cta-home": {
    "title": "[Home] Slider CTA",
    "props": {
      "itemsPerPage": {
        "desktop": 2,
        "tablet": 2,
        "phone": 1
      },
      "infinite": true,
      "showNavigationArrows": "never",
      "showPaginationDots": "never",
      "blockClass": ["container"]
    },
    "children": ["flex-layout.row#cta-1", "flex-layout.row#cta-2"]
  },
  "flex-layout.row#cta-1": {
    "props": {
      "blockClass": ["cta"]
    },
    "children": ["image#cta-1"]
  },
  "flex-layout.row#cta-2": {
    "props": {
      "blockClass": ["cta"]
    },
    "children": ["image#cta-2"]
  },
  "image#cta-1": {
    "title": "[Home] - CTA 1 - Imagem",
    "props": {
      "link": {
        "url": "/Mochilas?map=c"
      },
      "src": "assets/images/cta/call-to-action-mochila.jpg"
    }
  },
  "image#cta-2": {
    "title": "[Home] - CTA 2 - Imagem",
    "props": {
      "link": {
        "url": "/Hats?map=c"
      },
      "src": "assets/images/cta/call-to-action-hat.jpg"
    }
  }
}
